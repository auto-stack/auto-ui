type LeftPane as View {
    msg str

    fn view() {
        col {
            label(self.msg) {}
        }
    }
}

type MiddlePane as View {
    msg str

    fn view() {
        col {
            label(self.msg) {}
        }
    }
}

fn main() {
    app("Docks Example") {
        dock {
            left {
                leftPane() {}
            }
            middle {
                middlePane() {}
            }
        }
    }
}